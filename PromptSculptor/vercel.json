{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(favicon.ico|landing.html|index.html)",
      "destination": "/$1"
    },
    {
      "source": "/app",
      "destination": "/index.html"
    },
    {
      "source": "/app/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/",
      "destination": "/landing.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}